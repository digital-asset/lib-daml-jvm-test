daml 1.2
module Test where

import PingPong

testSetup = scenario $ do
  alice <- getParty "Alice"
  bob <- getParty "Bob"
  charlie <-getParty "Charlie"
  alice `submit` do
    create Ping with
        sender = alice
        receiver = bob
        count = 2